@page "{from}"
@model PageModelTest.Pages.ChangeModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<html>
	<head>
		<title>Learning ASP.NET Core</title>
	</head>

	<body>
		<h1>Welcome @RouteData.Values["from"]</h1>
		<form method="post">
			<div><b>New Comment</b></div>
			<div><textarea required="required" asp-for="Feedback.Comment"></textarea></div>
			<div style="padding-top:1em"><input type="submit" value="Update"/></div>
		</form>
	</body>
</html>
